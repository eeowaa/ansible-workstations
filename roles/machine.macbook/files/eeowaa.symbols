// vim: filetype=xkb
// TODO: Name this file in such a way as to avoid the above

partial modifier_keys
xkb_symbols "macbook" {

    // Swap Left Command with Left Control
    include "ctrl(swap_lwin_lctl)"

    // Swap Right Option with Right Command
    include "altwin(swap_ralt_rwin)"

    // Disable Left Option
    key <LALT> { [ VoidSymbol ] };
}
